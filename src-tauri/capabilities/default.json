{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:allow-execute",
    "shell:allow-open",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-remove",
    "fs:allow-mkdir",
    "fs:allow-rename",
    "fs:allow-copy-file",
    "fs:allow-exists",
    "fs:allow-read-dir",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-load"
  ]
}
